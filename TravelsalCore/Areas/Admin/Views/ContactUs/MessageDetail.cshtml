@model EntityLayer.Concrete.ContactUs
@{
    ViewData["Title"] = "MessageDetail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>MessageDetail</h3>
<table class="table table-bordered">
    <tr>
        <th>Sender</th>
        <th>Subject</th>
        <th>Date</th>
        <th>Message</th>
    </tr>

        <tr>
            <td>@Model.Name</td>
            <td>@Model.Subject</td>
            <td>@Model.MessageDate.ToString("dd.MMM.yyyy")</td>
            <td>@Model.MessageBody</td>
        </tr>
</table>

